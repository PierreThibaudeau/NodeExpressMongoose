<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <% if(typeof created !== 'undefined'){ %>
        <%= created %>
    <% } %>

    <% if(typeof list !== 'undefined'){ %>
        <% list.forEach(function(item){ %>
                <%= item %>
                <br>
            <% }) %>
    <% } %>
  </body>
</html>
